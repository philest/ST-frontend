  <div class="wrapper">
      <div class="body" style="vertical-align: top;">
        
      
        <div class="container" style="margin-top:65px;">
          <div class="pure-g">
              <div class="pure-u-1 class-information password-text">
                <h1 class="" style="display: inline-block;font-weight:600;font-size:22px;"><%= text[:header] %></h1>
                <img style="display: inline-block; padding-left: 11px;" src="/images/icons/lock-key.png" alt="">
                <!-- <p><%= text[:subtitle] %></p> -->
              </div>


              <div class="pure-u-1 login-form">
                <form id="teacher-info" action='/register/password' method="post"> 
                  <div class="signup-setup">
                    <div class="field" id="signup-field">
                      <label class="label"><%= text[:label] %></label>
                      <input type="password" name="password" placeholder="<%= text[:placeholder] %>" class="input" required>
                    </div>
                   
                      <button id="join" class="password-button signature-modal signup-form-button pure-button pure-button-primary get_update dialog-button"><%= text[:button] %></button>
                  </div>
                </form>
              </div>

          </div> <%# end pure-g %>

        </div>


      </div> <%# body class %>

  </div> <%# wrapper %>

<footer>
     <script>
        mixpanel.track_forms('#teacher-info', 'Set Password');
    </script>
</footer>
    



